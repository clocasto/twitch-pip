  "scripts": {
    "redis:dev": "redis-server server/env/modes/development/redis.conf",
    "redis:prod": "redis-server server/env/modes/production/redis.conf",
    "build:dev": "npm run clean&& npm run redis:dev & export NODE_ENV=development && webpack -d --watch --display-error-details",
    "build:prod": "npm run clean&& npm run redis:prod & export NODE_ENV=production && webpack -p",
    "clean": "rm -rf public/dist && mkdir public/dist",
    "deploy": "npm run build:prod && export NODE_ENV=production && node app.js",
    "start": "npm run build:dev & export NODE_ENV=development && nodemon app.js"
  }
